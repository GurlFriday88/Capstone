
@model IList<Capstone.Models.Prefix>


  <h2>BCBS Prefix List</h2>

    <table class="table">

        <tr>
            <th>Alpha Prefix</th>
            <th>Location</th>
            <th>Action</th>
        </tr>

        @foreach (Prefix prefix in Model )
        {
            <tr>
                <td>@prefix.Name</td>
                <td>@prefix.Provider.Name</td>
                <td><a class= "btn btn-sm btn-primary"   asp-action ="Delete" asp-route-id="@prefix.ID" >Delete</a></td>



            </tr>
        }
    </table>

    @if (Model.Count == 0)
{
    <p>Add some prefixes</p>
} 
