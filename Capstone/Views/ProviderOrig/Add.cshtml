<!-- beginning of form  -->
<!-- formlayout -->
<div class="container">
    <div class="row">
        <div class="col-sm-1"></div>
        <div class="col-sm-10">
            <!-- box beginning -->
            <div class="box box-solid box-primary">
                <div class="box-header with-border">
                    <h3 class="box-title">Create Provider</h3>
                    <div class="box-tools pull-right">
                        <!-- Buttons, labels, and many other things can be placed here! -->
                        <!-- Here is a label for example -->

                    </div>
                    <!-- /.box-tools -->
                </div>
                <!-- /.box-header -->
                <div class="box-body">

                    <!-- form start -->
                    <form>
                        <h2>Information</h2>
                        <div class=" information form-row">
                            <div class="form-group col-sm-6">
                                <label for="name" class="control-label ">Name</label>

                                <div>
                                    <input type="text" class="form-control" placeholder="Enter name">

                                </div>
                            </div>
                            <div class=" form-group col-sm-6">
                                <label for="" class="control-label">Phone Number</label>
                                <div>
                                    <input type="text" class="form-control" placeholder="Enter Phone number">
                                </div>

                            </div>
                        </div>

                        <h2>Required Documents</h2>

                        <div class="requiredDoc form-row">
                            <div class=" form-group col-sm-2">

                                <label for="#" class="control-label">Pages to save</label>
                                <input class="form-control" type="text" name="Savedpages" value="1" />

                            </div>
                            <div class="form-group col-sm-10">
                                <label for="#" class="control-label">Pages Description</label>
                                <textarea class="form-control" name="#" id="#" rows="2">Enter a description of each saved page</textarea>
                            </div>
                        </div>

                        <h2>Renewals</h2>
                        <div class=" renewal form-row">
                            <div class="form-group col-sm-6">
                                <label for="name" class="control-label ">Subscriber ID</label>

                                <div>
                                    <input type="text" class="form-control" placeholder="E.g P000000">

                                </div>
                            </div>
                            <div class=" form-group col-sm-6">
                                <label for="" class="control-label">Benefit Renewal</label>
                                <div>
                                    <input type="text" class="form-control" placeholder="E.g Every January">
                                </div>

                            </div>

                        </div>
                        <h2>Eligibility</h2>
                        <div class=" eligibility form-group row">

                            <label for="#" class="col-sm-2 col-form-label">Adults</label>



                            <div class="col-sm-10">

                                <div class="row">
                                    <div class="form-group col-sm-3">
                                        <label for="sel1" class="control-label ">Exam:</label>
                                        <select class="form-control" id="sel1">
                                            <option>Every 12 Months</option>
                                            <option>Every 24 Months</option>
                                            <option>None</option>

                                        </select>

                                    </div>
                                    <div class="form-group col-sm-3">
                                        <label for="sel1" class="control-label ">Frames:</label>
                                        <select class="form-control" id="sel1">
                                            <option>Every 12 Months</option>
                                            <option>Every 24 Months</option>
                                            <option>None</option>
                                        </select>

                                    </div>

                                    <div class="form-group col-sm-3">
                                        <label for="sel1" class="control-label">Lenses:</label>
                                        <select class="form-control" id="sel1">
                                            <option>Every 12 Months</option>
                                            <option>Every 24 Months</option>
                                            <option>None</option>
                                        </select>

                                    </div>

                                    <div class="form-group col-sm-3 ">
                                        <label for="sel1" class="control-label">Contacts:</label>
                                        <select class="form-control" id="sel1">
                                            <option>Every 12 Months</option>
                                            <option>Every 24 Months</option>
                                            <option>None</option>
                                        </select>
                                    </div>

                                </div>
                            </div>


                        </div>




                        <div class=" eligibility form-group row">

                            <label for="#" class="col-sm-2 col-form-label">Kids</label>

                            <div class="col-sm-10">

                                <div class="row">
                                    <div class="form-group col-sm-3">
                                        <label for="sel1" class="control-label ">Exam:</label>
                                        <select class="form-control" id="sel1">
                                            <option>Every 12 Months</option>
                                            <option>Every 24 Months</option>
                                            <option>None</option>

                                        </select>

                                    </div>
                                    <div class="form-group col-sm-3">
                                        <label for="sel1" class="control-label ">Frames:</label>
                                        <select class="form-control" id="sel1">
                                            <option>Every 12 Months</option>
                                            <option>Every 24 Months</option>
                                            <option>None</option>
                                        </select>

                                    </div>

                                    <div class="form-group col-sm-3">
                                        <label for="sel1" class="control-label">Lenses:</label>
                                        <select class="form-control" id="sel1">
                                            <option>Every 12 Months</option>
                                            <option>Every 24 Months</option>
                                            <option>None</option>
                                        </select>

                                    </div>

                                    <div class="form-group col-sm-3 ">
                                        <label for="sel1" class="control-label">Contacts:</label>
                                        <select class="form-control" id="sel1">
                                            <option>Every 12 Months</option>
                                            <option>Every 24 Months</option>
                                            <option>None</option>
                                        </select>
                                    </div>

                                </div>
                            </div>


                        </div>




                        <h2>Notes</h2>

                        <div class=" Notes form-row">
                            <div class="nav-tabs-custom">
                                <ul class="nav nav-tabs">
                                    <li class="active">
                                        <a href="#tab_1" data-toggle="tab" aria-expanded="true">Auth Note</a>
                                    </li>
                                    <li class="">
                                        <a href="#tab_2" data-toggle="tab" aria-expanded="false">Misc Notes</a>
                                    </li>

                                </ul>
                                <div class="tab-content">
                                    <div class="tab-pane active" id="tab_1">
                                        <textarea class="form-control" name="#" id="#" rows="5">Add your Auth Note here</textarea>




                                    </div>
                                    <div class="tab-pane" id="tab_2">

                                        <textarea class="form-control" name="#" id="#" rows="5">Add your Misc notes here</textarea>


                                    </div>

                                </div>
                            </div>
                        </div>



                    </form>
                    <!-- end form  -->
                    <!-- /.box-body -->
                </div>
                <div class="box-footer">
                    <input type="submit" class="btn btn-info" value="Submit Button">
                    <a href="#" class="btn btn-info" role="button">Cancel</a>


                </div>
                <!-- box-footer -->
            </div>

        </div>
        <!-- /.box -->

    </div>
    <div class="col-sm-1"></div>
</div>


